body
  font-size: 16px

section
  padding-top: 60px
.stand-out
  font-weight: bold
  
#branding-links
  float: right
  padding-top: 5px

.popover > .inner
  width: 430px

.summary
  margin-bottom: 10px

img.mug-shot
  border: 0.5em solid #FFFFFF

.row.header-row
  font-size: 32px
  line-height: 40px
  h1
    font-size: 40px

img.resume-mugshot
  margin-left: 200px
  display: inline-block
.marg-top
  margin-top: 10px
.resume-container
  background-color: #EEEEEE
  border-top: 1px solid #E5E5E5
  box-shadow: 0 5px 15px rgba(0, 0, 0, 0.024) inset
  padding: 30px 
  text-shadow: 0 1px 0 #FFFFFF

  .resume-content
    margin: 0 20px
  em
    margin-bottom: 10px
    display: inline-block
  h2
    color: #999999
    font-size: 1.25em
    font-variant: small-caps
    letter-spacing: 0.2em
